{
  "name": "kwagner-dev",
  "private": true,
  "workspaces": [
    "/home",
    "/wedding",
    "/components/SEO"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "dev": "concurrently \"npm run dev -w home\" \"npm run dev -w wedding\"",
    "dev:home": "npm run dev -w home",
    "dev:wedding": "npm run dev -w wedding",
    "install:all": "npm install",
    "test": "npm run test --workspaces --if-present"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "optionalDependencies": {
    "@rspack/binding-linux-x64-gnu": "^1.6.3"
  }
}
