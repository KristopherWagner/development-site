{
  "name": "kwagner-monorepo",
  "private": true,
  "workspaces": [
    "/home",
    "/wedding"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "concurrently \"npm run dev -w home\" \"npm run dev -w wedding\"",
    "dev:home": "npm run dev -w home",
    "dev:wedding": "npm run dev -w wedding",
    "install:all": "npm install",
    "test": "npm run test --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "optionalDependencies": {
    "@rspack/binding-linux-x64-gnu": "^1.6.3"
  }
}
